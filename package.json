{
  "name": "mathlang",
  "version": "1.0.0",
  "description": "test math vm",
  "main": "dest/mathlang.js",
  "dependencies": {
    "@stdlib/stdlib": "0.0.90",
    "@types/node": "^11.12.1",
    "big-integer": "^1.6.43",
    "chevrotain": "^4.3.1",
    "convert-units": "^2.3.4",
    "decimal.js": "^10.1.1",
    "systemjs": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
