    "test-ast-if": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/2-ast-builder/math_lang_parser_3_if.test.ts",
    "test-ast-switch": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/2-ast-builder/math_lang_parser_3_switch.test.ts",
    "test-ast-assign": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/2-ast-builder/math_lang_parser_3_assign.test.ts",
