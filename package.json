  "scripts": {
    "test": "mocha --reporter spec --require ts-node/register src/**/*.test.ts",
    "test-compiler": "mocha --reporter spec --require ts-node/register src/tests/integration/compiler.test.ts",
    "test-compiler-all": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/**/*.test.ts",
    "test-ast": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/2-ast-builder/math_lang_parser_*.test.ts",
