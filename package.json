    "test-ast-module": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/2-ast-builder/math_lang_parser_4_module*.test.ts",
    "test-ic": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_*.test.ts",
    "test-ic-const": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_module_const.test.ts",
    "test-ic-func-1": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_1.test.ts",
    "test-ic-func-2": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_2.test.ts",
    "test-ic-func-3": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_3.test.ts",
    "test-ic-func-4": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_4.test.ts",
    "test-ic-func-5": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_5.test.ts",
    "test-ic-func-6": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_functions_6.test.ts",
    "test-ic-type-1": "mocha --reporter spec --require ts-node/register src/tests/unit/compiler/3-ic-builder/math_lang_ic_types_1.test.ts",
    "test-vm": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/*.test.ts",
    "test-vm-instr1": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/engine_instructions-1.test.ts",
    "test-vm-instr2": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/engine_instructions-2.test.ts",
    "test-vm-instr3": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/engine_instructions-3.test.ts",
    "test-vm-reg": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/engine_registers.test.ts",
    "test-vm-stack": "mocha --reporter spec --require ts-node/register src/tests/unit/engine/engine_stack.test.ts",
    "testjs": "mocha  dest/tests/unit/**/*.test.js",
    "test-yacas": "node src/vmachine/javascript/cas/import_yacas.js",
    "watch-test": "mocha --reporter spec --require ts-node/register --watch --watch-extensions ts src/**/*.test.ts",
    "asbuild-vm:untouched": "asc src/vmachine/assemblyscript/assembly/index.ts -b src/vmachine/assemblyscript/build/untouched.wasm -t src/vmachine/assemblyscript/build/untouched.wat --sourceMap --validate --debug",
    "asbuild-vm:optimized": "asc src/vmachine/assemblyscript/assembly/index.ts -b src/vmachine/assemblyscript/build/optimized.wasm -t src/vmachine/assemblyscript/build/optimized.wat --sourceMap --validate --optimize",
    "asbuild-vm": "npm run asbuild-vm:untouched && npm run asbuild-vm:optimized"
  },
